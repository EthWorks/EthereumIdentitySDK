{
  "name": "@universal-login/react",
  "version": "0.4.2",
  "main": "dist/cjs/src/index.js",
  "types": "dist/cjs/src/index.d.ts",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "start": "webpack-dev-server --mode development --progress",
    "lint": "eslint '{src,test}/**/*.{ts,tsx}'",
    "clean": "rimraf dist",
    "build": "yarn build:cjs",
    "build:cjs": "tsc --outDir dist/cjs/ -p tsconfig.json && cpx \"src/ui/assets/**/*\" dist/cjs/src/ui/assets && cpx \"src/ui/styles/**/*\" dist/cjs/src/ui/styles",
    "test": "yarn build && yarn test:nobuild",
    "test:nobuild": "cross-env NODE_ENV=test mocha --require ./dist/cjs/test/testconfig -t 10000 --exit \"dist/cjs/test/**/*.js\"",
    "start:dev": "ts-node scripts/dev-start.ts"
  },
  "dependencies": {
    "@ramp-network/ramp-instant-sdk": "1.5.1",
    "@restless/sanitizers": "^0.2.2",
    "@universal-login/commons": "0.4.2",
    "@universal-login/sdk": "0.4.2",
    "dotenv": "^8.1.0",
    "ethers": "^4.0.39",
    "lodash.range": "^3.2.0",
    "node-sass": "^4.10.0",
    "react-blockies": "^1.4.1",
    "react-qr-svg": "^2.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.2.2",
    "react-use-mutable": "^0.1.0",
    "reactive-properties": "^0.1.5",
    "use-subscription": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.x",
    "react-dom": "^16.8.x"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/lodash.range": "^3.2.6",
    "@types/mocha": "^5.2.5",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@universal-login/relayer": "0.4.2",
    "canvas": "^2.5.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^5.0.3",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "ethereum-waffle": "^2.1.2",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "nock": "^11.3.5",
    "node-fetch": "^2.6.0",
    "puppeteer": "1.19.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
