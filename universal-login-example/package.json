{
  "name": "@universal-login/example",
  "version": "0.3.30",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@universal-login/commons": "^0.3.30",
    "@universal-login/relayer": "^0.3.30",
    "@universal-login/sdk": "^0.3.30"
  },
  "scripts": {
    "start": "parcel --no-cache ./src/index.html",
    "example:build": "parcel build ./src/index.html",
    "prebuild": "yarn clean",
    "build": "yarn contracts:build && yarn relayer:build && parcel build ./src/index.html",
    "lint": "eslint src/**/*.js src/**/*.jsx scripts/**/*.js test/**/*.js",
    "test": "cross-env NODE_ENV=test mocha --recursive --require ./test/helpers/testNullCompiler.js --require babel-register test -t 10000",
    "clean": "rimraf .cache build dist",
    "contracts:build": "waffle .waffle.js",
    "contracts:deploy": "babel-node scripts/deployContracts.js",
    "start:dev": "babel-node scripts/dev-start.js",
    "relayer:build": "babel src/relayer --out-dir dist/relayer",
    "relayer:start": "node dist/relayer/startRelayer.js"
  },
  "devDependencies": {
    "@universal-login/ops": "^0.3.30"
  },
  "engines": {
    "node": ">=10.9"
  }
}
